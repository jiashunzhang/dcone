<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dcone.dtss.model.UserMapper">
    <select id="selectUserByID" parameterType="int" resultType="com.dcone.dtss.model.dc_user">
        select * from `dc_user` where uid = #{uid}
    </select>
</mapper>